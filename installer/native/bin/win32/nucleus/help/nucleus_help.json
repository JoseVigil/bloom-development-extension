  {
    "name": "launch",
    "use": "launch [profile_id]",
    "short": "Launch a browser instance for a profile using Sentinel",
    "category": "ORCHESTRATION",
    "args": [
      {
        "name": "profile_id",
        "required": false
      }
    ],
    "options": [
      {
        "name": "account",
        "usage": "Account identifier"
      },
      {
        "name": "alias",
        "usage": "Profile alias"
      },
      {
        "name": "config",
        "usage": "JSON config file path or '-' for stdin"
      },
      {
        "name": "email",
        "usage": "Email address"
      },
      {
        "name": "extension",
        "usage": "Extension to load"
      },
      {
        "name": "heartbeat",
        "usage": "Enable heartbeat tracking",
        "default": "false"
      },
      {
        "name": "mode",
        "usage": "Launch mode (landing, discovery, etc.)",
        "default": "landing"
      },
      {
        "name": "register",
        "usage": "Register new profile",
        "default": "false"
      },
      {
        "name": "role",
        "usage": "User role"
      },
      {
        "name": "save",
        "usage": "Save configuration for future use",
        "default": "false"
      },
      {
        "name": "service",
        "usage": "Service identifier"
      },
      {
        "name": "step",
        "usage": "Execution step"
      }
    ],
    "example": "  nucleus synapse launch profile_001\n  nucleus synapse launch profile_001 --email test@mail.com --service google\n  nucleus synapse launch profile_001 --mode discovery --save\n  nucleus --json synapse launch profile_001 --config launch.json\n\nJSON Output:\n  {\n    \"success\": true,\n    \"profile_id\": \"profile_001\",\n    \"launch_id\": \"launch_abc123\",\n    \"chrome_pid\": 9876,\n    \"debug_port\": 9222,\n    \"extension_loaded\": true,\n    \"state\": \"RUNNING\",\n    \"timestamp\": 1707418080\n  }"
  },
